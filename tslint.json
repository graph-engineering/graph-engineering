{
  "extends": "./.tst/tslint.json",
  "rules": {
    "no-expression-statement": [
      true,
      { "ignore-prefix": ["main", "describe", "test", "expect"] }
    ],
    "no-duplicate-imports": {
      "severity": "error",
      "options": {
        "allow-namespace-imports": true
      }
    },
    "array-type": false
  }
}
