{
  "name": "graph-engineering",
  "private": true,
  "scripts": {
    "tst:init": "tst init --install false",
    "tst:scripts": "tst scripts",
    "tst:deps": "tst deps",
    "adapters:test": "tst test adapters",
    "adapters:test:watch": "tst test adapters --watch",
    "connection:test": "tst test connection",
    "connection:test:watch": "tst test connection --watch",
    "connection:dev": "lerna exec --scope @grapheng/connection -- npm run start",
    "connection:build": "lerna exec --scope @grapheng/connection -- npm run build",
    "connection:pack": "lerna exec --scope @grapheng/connection -- npm pack",
    "graphql-extra:test": "tst test graphql-extra",
    "graphql-extra:test:watch": "tst test graphql-extra --watch",
    "graphql-extra:build": "lerna exec --scope @grapheng/graphql-extra -- npm run build",
    "time:test": "tst test time",
    "time:test:watch": "tst test time --watch",
    "time:build": "npm run time:test && lerna exec --scope @grapheng/time -- npm run build",
    "time:dev1": "lerna exec --scope @grapheng/time -- npm run example1",
    "time:dev2": "lerna exec --scope @grapheng/time -- npm run example2",
    "prelude:test": "lerna exec --scope @grapheng/prelude -- npm run test",
    "prelude:test:watch": "lerna exec --scope @grapheng/prelude -- npm run test:watch"
  },
  "dependencies": {
    "typescript": "^3.6.2",
    "typescript-tooling": "^4.1.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/node": "^10.14.6",
    "jest": "^24.8.0",
    "lerna": "^3.13.4",
    "nodemon": "^1.19.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-typescript": "^1.0.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-security": "^1.16.0",
    "tslint-immutable": "^5.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graph-engineering/graph-engineering.git"
  },
  "bugs": {
    "url": "https://github.com/graph-engineering/graph-engineering/issues"
  },
